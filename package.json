{
    "name": "checkout-node-sdk",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-typescript": "^7.3.3",
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.6",
        "@types/node-fetch": "^2.3.2",
        "chai": "^4.2.0",
        "codecov": "^3.1.0",
        "mocha": "^5.2.0",
        "node-fetch": "^2.3.0",
        "ts-node": "^8.1.0",
        "typescript": "^3.5.1"
    },
    "scripts": {
        "build": "tsc",
        "build-watch": "tsc --watch",
        "lint": "tslint './src/**/*.{ts}' -t verbose",
        "lint-fix": "tslint --fix './src/**/*.{ts}' -t verbose",
        "test": "nyc --reporter=text mocha --timeout 10000 -r ts-node/register test/**/*test.ts",
        "test:watch": "mocha --timeout 20000 --require ts-node/register --watch-extensions ts --watch 'test/**/*test.ts'",
        "posttest": "nyc report --reporter=json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ioan-ghisoi-cko/checkout-node-sdk.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ioan-ghisoi-cko/checkout-node-sdk/issues"
    },
    "homepage": "https://github.com/codecov/example-typescript#readme",
    "dependencies": {
        "@types/jquery": "^3.3.29",
        "@types/nock": "^10.0.2",
        "lodash": "^4.17.11",
        "nock": "^10.0.6",
        "nyc": "^14.1.1"
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "extension": [
            ".ts",
            ".tsx"
        ],
        "require": [
            "ts-node/register"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true
    }
}